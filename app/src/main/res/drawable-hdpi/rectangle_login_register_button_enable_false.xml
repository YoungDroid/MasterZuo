<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="@color/login_register_gray" />

    <!-- 大小 -->
    <size
        android:width="100dp"
        android:height="45dp" />

    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="0dp"
        android:color="@color/login_register_gray" />

    <!-- 矩形的圆角半径 -->
    <corners android:radius="6dp" />
</shape>