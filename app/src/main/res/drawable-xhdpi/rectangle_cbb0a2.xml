<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="#cbb0a2" />

    <!-- 大小 -->
    <size
        android:width="52.8dp"
        android:height="28.8dp" />

    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="0dp"
        android:color="#32000000" />

    <!-- 矩形的圆角半径 -->
    <corners android:radius="3.6dp" />
</shape>