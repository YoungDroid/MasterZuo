<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!-- 填充颜色 -->
    <solid android:color="#f7f7f7" />

    <!-- 大小 -->
    <size
        android:width="5dp"
        android:height="5dp" />

    <!-- 线的宽度，颜色灰色 -->
    <stroke
        android:width="0dp"
        android:color="#32000000" />

    <!-- 矩形的圆角半径 -->
    <corners android:radius="2.5dp" />
</shape>